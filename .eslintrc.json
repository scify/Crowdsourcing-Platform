{
  "env": {
    "browser": true,
    "es2021": true,
    "jquery": true,
    "node": true
  },
  "globals": {
    "_": true,
    "axios": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:vue/vue3-essential",
    "prettier"
  ],
  "overrides": [
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "vue"
  ],
  "rules": {
    "vue/no-use-v-if-with-v-for": "off",
    "indent": [
      "error",
      "tab",
      {
        "SwitchCase": 1,
        "ignoredNodes": ["ConditionalExpression"]
      }
    ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    "semi": [
      "error",
      "always"
    ]
  }
}
